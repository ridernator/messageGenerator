<?xml version="1.0" encoding="UTF-8"?>

<structure name="Blob" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Structure.xsd">
    <documentation>This class is the blob class</documentation>
    
    <constants>
		<constant name="SIZE_OF_ARRAY" type="unsigned_int_32" value="10000">
			<documentation>Size of the "intArray" array</documentation>
		</constant>
    </constants>
      
    <elements>  
        <element name="fatArray" type="array" arraySize="SIZE_OF_ARRAY">
            <documentation>This type is called fatArray</documentation>
            <subElement type="float_64"/>
        </element>
    </elements>
</structure>